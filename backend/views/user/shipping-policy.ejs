            font-size: 1.5rem;
            color: var(--theme-color);
        }

        .terms-conditions-contact-text {
            margin-bottom: 10px;
            font-size: 1.1rem;
        }

        .terms-conditions-highlight-box {
            background-color: #f0f4f8;
            border-left: 4px solid var(--theme-color);
            padding: 20px;
            margin: 25px 0;
            border-radius: 0 8px 8px 0;
        }

        .terms-conditions-warning-box {
            background-color: #fff8e1;
            border: 2px solid #ffa726;
            border-radius: 8px;
            padding: 20px;
            margin: 25px 0;
        }

        .terms-conditions-footer-note {
            border-top: 3px solid var(--theme-color);
            padding-top: 30px;
            text-align: center;
            margin-top: 60px;
        }

        .terms-conditions-footer-text {
            font-style: italic;
            font-size: 1rem;
            opacity: 0.8;
        }

        .terms-conditions-numbered-list {
            counter-reset: section-counter;
            list-style: none;
            padding-left: 0;
        }

        .terms-conditions-numbered-item {
            counter-increment: section-counter;
            margin-bottom: 15px;
            font-size: 1.5rem !important;
            line-height: 1.7;
        }

        .terms-conditions-numbered-item::before {
            content: counter(section-counter) ". ";
            font-weight: bold;
            color: var(--theme-color);
        }

        @media (max-width: 768px) {
