    <style>
        :root {
            --theme-color: #001F3F;
            --background-color: white;
        }

        .terms-conditions-wrapper {
            max-width: 1200px;
            margin: 0 auto;


            line-height: 1.6;
            color: var(--theme-color);
            background-color: var(--background-color);
        }

        .terms-conditions-title {
            text-align: center;
            margin-bottom: 60px;
            border-bottom: 3px solid var(--theme-color);
            padding-bottom: 30px;
        }

        .terms-conditions-main-heading {
            font-size: 3.5rem;
            font-weight: 400;
            letter-spacing: 2px;
            text-transform: uppercase;
            margin-bottom: 20px;
            color: var(--theme-color);
        }

        .terms-conditions-company-name {
            font-size: 2.4rem;
            font-style: italic;
            margin-bottom: 15px;
            color: var(--theme-color);
            opacity: 0.9;
        }

        .terms-conditions-updated {
            font-style: italic;
            font-size: 1.1rem;
            color: var(--theme-color);
            opacity: 0.8;
        }

        .terms-conditions-section {
            margin-bottom: 50px;
        }

        .terms-conditions-section-title {
            font-size: 3rem;
            font-weight: 500;
            margin-bottom: 25px;
            color: var(--theme-color);
            border-left: 5px solid var(--theme-color);
            padding-left: 20px;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .terms-conditions-subsection-title {
            font-size: 2rem;
            font-weight: 500;
            margin: 30px 0 15px 0;
            color: var(--theme-color);
            text-transform: capitalize;
        }

        .terms-conditions-text {
            font-size: 1.5rem;
            margin-bottom: 20px;
            text-align: justify;
            line-height: 1.8;
        }

        .terms-conditions-list {
            margin: 20px 0;
            padding-left: 40px;
        }

        .terms-conditions-list-item {
            font-size: 1.5rem;
            margin-bottom: 12px;
            line-height: 1.7;
        }

        .terms-conditions-contact-box {
            background-color: #f8f9fa;
            border: 2px solid var(--theme-color);
            border-radius: 8px;
            padding: 30px;
            margin: 40px 0;
            text-align: center;
        }

        .terms-conditions-contact-title {
            margin-bottom: 20px;
            font-size: 1.5rem;
            color: var(--theme-color);
        }

        .terms-conditions-contact-text {
            margin-bottom: 10px;
            font-size: 1.1rem;
        }

        .terms-conditions-highlight-box {
            background-color: #f0f4f8;
            border-left: 4px solid var(--theme-color);
            padding: 20px;
            margin: 25px 0;
            border-radius: 0 8px 8px 0;
        }

        .terms-conditions-warning-box {
            background-color: #fff8e1;
            border: 2px solid #ffa726;
            border-radius: 8px;
            padding: 20px;
            margin: 25px 0;
        }

        .terms-conditions-footer-note {
            border-top: 3px solid var(--theme-color);
            padding-top: 30px;
            text-align: center;
            margin-top: 60px;
        }

        .terms-conditions-footer-text {
            font-style: italic;
            font-size: 1rem;
            opacity: 0.8;
        }

        .terms-conditions-numbered-list {
            counter-reset: section-counter;
            list-style: none;
            padding-left: 0;
        }

        .terms-conditions-numbered-item {
            counter-increment: section-counter;
            margin-bottom: 15px;
            font-size: 1.5rem !important;
            line-height: 1.7;
        }

        .terms-conditions-numbered-item::before {
            content: counter(section-counter) ". ";
            font-weight: bold;
            color: var(--theme-color);
        }

        @media (max-width: 768px) {
            .terms-conditions-wrapper {
                padding: 40px 20px;
            }

            .terms-conditions-main-heading {
                font-size: 2rem;
            }

            .terms-conditions-section-title {
                font-size: 1.5rem;
            }

            .terms-conditions-text,
            .terms-conditions-list-item {
                font-size: 1rem;
            }
        }
    </style>
</head>

