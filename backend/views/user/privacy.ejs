    <style>
        :root {
            --theme-color: #001F3F;
            --background-color: white;
        }
        
        .privacy-policy-wrapper {
            max-width: 1200px;
            margin: 0 auto;
          
   
            line-height: 1.6;
            color: var(--theme-color);
            background-color: var(--background-color);
        }
        
        .privacy-policy-title {
            text-align: center;
            margin-bottom: 60px;
            border-bottom: 3px solid var(--theme-color);
            padding-bottom: 30px;
        }
        
        .privacy-policy-main-heading {
            font-size: 3.5rem;
            font-weight: 400;
            letter-spacing: 2px;
            text-transform: uppercase;
            margin-bottom: 20px;
            color: var(--theme-color);
        }
        
        .privacy-policy-updated {
            font-style: italic;
            font-size: 1.1rem;
            color: var(--theme-color);
            opacity: 0.8;
        }
        
        .privacy-policy-section {
            margin-bottom: 50px;
        }
        
        .privacy-policy-section-title {
            font-size: 3rem;
            font-weight: 500;
            margin-bottom: 25px;
            color: var(--theme-color);
            border-left: 5px solid var(--theme-color);
            padding-left: 20px;
            text-transform: uppercase;
            letter-spacing: 1px;
        }
        
        .privacy-policy-subsection-title {
            font-size: 2rem;
            font-weight: 500;
            margin: 30px 0 15px 0;
            color: var(--theme-color);
            text-transform: capitalize;
        }
        
        .privacy-policy-text {
            font-size: 1.5rem;
            margin-bottom: 20px;
            text-align: justify;
            line-height: 1.8;
        }
        
        .privacy-policy-list {
            margin: 20px 0;
            padding-left: 40px;
        }
        
        .privacy-policy-list-item {
            font-size: 1.5rem;
            margin-bottom: 12px;
            line-height: 1.7;
        }
        
        .privacy-policy-contact-box {
            background-color: #f8f9fa;
            border: 2px solid var(--theme-color);
            border-radius: 8px;
            padding: 30px;
            margin: 40px 0;
            text-align: center;
        }
        
        .privacy-policy-contact-title {
            margin-bottom: 20px;
            font-size: 1.5rem;
            color: var(--theme-color);
        }
        
        .privacy-policy-contact-text {
            margin-bottom: 10px;
            font-size: 1.1rem;
        }
        
        .privacy-policy-highlight-box {
            background-color: #f0f4f8;
            border-left: 4px solid var(--theme-color);
            padding: 20px;
            margin: 25px 0;
            border-radius: 0 8px 8px 0;
        }
        
        .privacy-policy-footer-note {
            border-top: 3px solid var(--theme-color);
            padding-top: 30px;
            text-align: center;
            margin-top: 60px;
        }
        
        .privacy-policy-footer-text {
            font-style: italic;
            font-size: 1rem;
            opacity: 0.8;
        }
        
        @media (max-width: 768px) {
            .privacy-policy-wrapper {
                padding: 40px 20px;
            }
            
            .privacy-policy-main-heading {
                font-size: 2rem;
            }
            
            .privacy-policy-section-title {
                font-size: 1.5rem;
            }
            
            .privacy-policy-text,
            .privacy-policy-list-item {
                font-size: 1rem;
            }
        }
    </style>
</head>
