            <div class="page-header">
                <h1 class="page-title" style="color: white;">My Orders</h1>
            </div>

            <div class="order-tabs" style="margin-bottom: 24px; display: flex; gap: 12px; flex-wrap: wrap;">
                <% 
                    const tab = typeof selectedTab !== 'undefined' ? selectedTab : (typeof queryTab !== 'undefined' ? queryTab : 'current');
                    function tabClass(name) { 
                        return tab === name 
                            ? 'btn btn-primary order-tab-btn-active' 
                            : 'btn btn-outline-primary order-tab-btn'; 
                    }
                %>
                <a href="/orders?tab=current" class="<%= tabClass('current') %>">Current Orders</a>
                <a href="/orders?tab=delivered" class="<%= tabClass('delivered') %>">Delivered Orders</a>
                <a href="/orders?tab=cancelled" class="<%= tabClass('cancelled') %>">Cancelled Orders</a>
            </div>

            <% if (orders.length === 0) { %>
                <div class="empty-state">
                    <div style="display: flex; justify-content: center; align-items: center; margin: 0 auto 20px; max-width: 320px;">           
                        <dotlottie-player src="https://lottie.host/7935dd8b-1312-462b-bf34-e2780ce03ff0/fZc43ONcnq.lottie" background="transparent" speed="1" style="width: 300px; height: 300px" loop autoplay></dotlottie-player>
                    </div>
                    <% if (tab === 'current') { %>
                        <h2 style="text-align: center; margin-bottom: 10px;">You have no current orders</h2>
                        <p style="text-align: center; color: #6b7280; margin-bottom: 20px;">When you place an order, it will appear here.</p>
                    <% } else if (tab === 'delivered') { %>
                        <h2 style="text-align: center; margin-bottom: 10px;">No delivered orders yet</h2>
                        <p style="text-align: center; color: #6b7280; margin-bottom: 20px;">Delivered orders will show up here.</p>
                    <% } else if (tab === 'cancelled') { %>
                        <h2 style="text-align: center; margin-bottom: 10px;">No cancelled orders</h2>
                        <p style="text-align: center; color: #6b7280; margin-bottom: 20px;">Cancelled orders will show up here.</p>
                    <% } %>
                    <div style="text-align: center;padding-bottom: 40px;">
                        <a href="/store" class="btn" style="background-color: #111827;color: white;">Continue Shopping</a>
                    </div>
                </div>
            <% } else { %>
                <div class="orders-list">
                    <% orders.forEach(order => { %>
                    <div class="order-card">
                        <div class="order-header">
                            <div class="order-meta">
                                <small>ORDER PLACED</small>
                                <p><%= order.orderDate %></p>
                            </div>
                            <div class="order-meta">
                                <small>TOTAL</small>
                                <p>₹<%= order.totalAmount.toLocaleString('en-IN') %></p>
                            </div>
                            <div class="order-meta">
                                <small>ORDER #<%= order.orderId || order._id.toString().slice(-6).toUpperCase() %></small>
                                <p>
                                    <span class="order-status status-<%= order.orderStatus.toLowerCase() %>">
                                        <%= order.orderStatus %>
                                    </span>
                                </p>
                            </div>
                        </div>
                        
                        <div class="order-body">
                            <% order.items.forEach(item => { %>
                            <div class="order-item">
                                <img src="<%= item.product?.images?.[0] || '/images/placeholder-product.jpg' %>" 
                                     alt="<%= item.name %>" class="item-image">
                                <div class="item-details">
                                    <h3 class="item-name"><%= item.name %></h3>
                                    <div class="item-meta">
                                        <% if (item.selectedColor) { %>Color: <%= item.selectedColor %><% } %>
                                        <% if (item.selectedSize) { %> | Size: <%= item.selectedSize %><% } %>
                                        | Qty: <%= item.quantity %>
                                    </div>
                                    <div class="item-price">₹<%= item.price.toLocaleString('en-IN') %></div>
                                </div>
                                <button class="btn view-details" style="padding: 2px 5px; cursor: pointer;" data-order-id="<%= order._id %>">
                                    View Details
                                </button>
                            </div>
                            <% }); %>
                            
                            <div class="order-actions">
                                <% if (tab === 'current' && order.canCancel) { %>
                                    <button class="btn btn-danger cancel-order" data-order-id="<%= order._id %>">
                                      Cancel Order
                                    </button>
                                  <% } %>
                                  <% if (tab === 'delivered' && order.canReturn) { %>
                                    <button class="btn btn-warning return-order" data-order-id="<%= order._id %>">
                                      Return/Replace Item
                                    </button>
                                    
                                    <!-- Replacement Modal -->
                                    <div class="modal fade" id="replacementModal" tabindex="-1" aria-hidden="true">
                                      <div class="modal-dialog">
                                        <div class="modal-content">
                                          <div class="modal-header">
                                            <h5 class="modal-title">Request Replacement</h5>
                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                          </div>
                                          <div class="modal-body">
                                            <form id="replacementForm">
                                              <input type="hidden" name="orderId" value="<%= order._id %>">
                                              <div class="mb-3">
                                                <label class="form-label">Select Item</label>
                                                <select name="itemId" class="form-select" required>
                                                  <% order.items.forEach(item => { %>
                                                    <option value="<%= item._id %>"><%= item.name %></option>
                                                  <% }); %>
                                                </select>
                                              </div>
                                              <div class="mb-3">
                                                <label class="form-label">Reason for Replacement</label>
                                                <select name="reason" class="form-select" required>
                                                  <option value="">Select a reason</option>
                                                  <option value="Damaged">Damaged Product</option>
                                                  <option value="WrongItem">Wrong Item Received</option>
                                                  <option value="Defective">Defective Product</option>
                                                  <option value="Other">Other</option>
                                                </select>
                                              </div>
                                              <div class="mb-3" id="otherReasonContainer" style="display:none;">
                                                <label class="form-label">Please specify</label>
                                                <textarea name="otherReason" class="form-control"></textarea>
                                              </div>
                                            </form>
                                          </div>
                                          <div class="modal-footer">
                                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                            <button type="button" class="btn btn-primary" id="submitReplacement">Submit Request</button>
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                                  <% } %>
                                  
                                  <script>
                                  document.addEventListener('DOMContentLoaded', function() {
                                    // Replacement request
                                    document.querySelectorAll('.return-order').forEach(button => {
                                      button.addEventListener('click', function() {
                                        const modal = new bootstrap.Modal(document.getElementById('replacementModal'));
                                        modal.show();
                                      });
                                    });
                                  
                                    // Show/hide other reason field
                                    document.querySelector('select[name="reason"]').addEventListener('change', function() {
                                      document.getElementById('otherReasonContainer').style.display = 
                                        this.value === 'Other' ? 'block' : 'none';
                                    });
                                  
                                    // Submit replacement request
                                    document.getElementById('submitReplacement').addEventListener('click', async function() {
                                      const form = document.getElementById('replacementForm');
                                      const formData = new FormData(form);
                                      const data = Object.fromEntries(formData.entries());
                                      
                                      if (data.reason === 'Other' && !data.otherReason) {
                                        alert('Please specify the reason for replacement');
                                        return;
                                      }
                                  
                                      this.disabled = true;
                                      this.innerHTML = '<span class="spinner-border spinner-border-sm"></span> Processing...';
                                  
                                      try {
                                        const response = await fetch('/orders/replace', {
                                          method: 'POST',
                                          headers: {
                                            'Content-Type': 'application/json'
                                          },
                                          body: JSON.stringify(data)
                                        });
                                  
                                        const result = await response.json();
                                        
                                        if (result.success) {
                                          alert('Replacement request submitted successfully!');
                                          window.location.reload();
                                        } else {
                                          throw new Error(result.message || 'Failed to submit replacement request');
                                        }
                                      } catch (error) {
                                        alert(error.message);
                                        this.disabled = false;
                                        this.innerHTML = 'Submit Request';
                                      }
                                    });
                                  });
                                  </script>
                                <a href="/store" class="btn" style="background-color: #111827;color: white;">
                                    Buy Again
                                </a>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                </div>
                <script>
                    document.addEventListener('DOMContentLoaded', function() {
                      // Order cancellation
                      document.querySelectorAll('.cancel-order').forEach(button => {
                        button.addEventListener('click', async function() {
                          if (!confirm('Are you sure you want to cancel this order?')) return;
                          
                          const orderId = this.dataset.orderId;
                          const spinner = '<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> Processing...';
                          this.innerHTML = spinner;
                          this.disabled = true;
                          
                          try {
                            const response = await fetch(`/orders/${orderId}/cancel`, {
                              method: 'POST',
                              headers: {
                                'Content-Type': 'application/json'
                              }
                            });
                            
                            const result = await response.json();
                            
                            if (result.success) {
                              alert('Order cancelled successfully. Refund will be processed shortly.');
                              window.location.reload();
                            } else {
                              throw new Error(result.message || 'Failed to cancel order');
                            }
                          } catch (error) {
                            alert(error.message);
                            this.innerHTML = 'Cancel Order';
                            this.disabled = false;
                          }
                        });
                      });
                    });
                    </script>
                <!-- Pagination -->
                <% if (totalPages > 1) { %>
                <ul class="pagination">
                    <% if (hasPrevPage) { %>
                        <li class="page-item">
                            <a class="page-link" href="/orders?page=<%= currentPage - 1 %>&tab=<%= tab %>">Previous</a>
                        </li>
                    <% } else { %>
                        <li class="page-item disabled">
                            <span class="page-link">Previous</span>
                        </li>
                    <% } %>
                    
                    <% for (let i = 1; i <= totalPages; i++) { %>
                        <li class="page-item <%= i === currentPage ? 'active' : '' %>">
                            <a class="page-link" href="/orders?page=<%= i %>&tab=<%= tab %>"><%= i %></a>
                        </li>
                    <% } %>
                    
                    <% if (hasNextPage) { %>
                        <li class="page-item">
                            <a class="page-link" href="/orders?page=<%= currentPage + 1 %>&tab=<%= tab %>">Next</a>
                        </li>
                    <% } else { %>
                        <li class="page-item disabled">
                            <span class="page-link">Next</span>
                        </li>
                    <% } %>
                </ul>
                <% } %>
            <% } %>
        </div>
    
        <!-- Order Details Modal -->
        <div id="orderDetailsModal" class="modal">
            <div class="modal-content">
                <div class="modal-header">
                    <h2 class="modal-title">Order Details #<span id="modalOrderId"></span></h2>
                    <button class="modal-close">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col">
                            <div class="modal-section">
                                <h3>Shipping Address</h3>
                                <div id="shippingAddress"></div>
                            </div>
                            
                            <div class="modal-section">
                                <h3>Payment Method</h3>
                                <p id="paymentMethod"></p>
                            </div>
                            
                            <div class="modal-section">
                                <h3>Order Summary</h3>
                                <div id="orderSummary"></div>
                            </div>
                        </div>
                        <div class="col">
                            <div class="modal-section">
                                <h3>Tracking Information</h3>
                                <div id="trackingInfo">
                                    <div class="tracking-progress">
                                        <div class="progress-bar-container">
                                            <div id="trackingProgress" class="progress-bar"></div>
                                        </div>
                                        <ul id="trackingSteps" class="tracking-steps"></ul>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="modal-section">
                                <h3>Coupon Applied</h3>
                                <p id="couponInfo">No coupon applied</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    
        <script>
            // Modal functionality
            const modal = document.getElementById('orderDetailsModal');
            const modalClose = modal.querySelector('.modal-close');
            
            // Close modal when clicking X or outside
            modalClose.addEventListener('click', () => modal.style.display = 'none');
            modal.addEventListener('click', (e) => {
                if (e.target === modal) modal.style.display = 'none';
            });
            
            // View order details
            document.querySelectorAll('.view-details').forEach(button => {
                button.addEventListener('click', async function() {
                    const orderId = this.getAttribute('data-order-id');
                    
                    try {
                        const response = await fetch(`/orders/${orderId}`);
                        if (!response.ok) throw new Error('Failed to fetch order details');
                        
                        const order = await response.json();
                        displayOrderDetails(order);
                    } catch (error) {
                        console.error('Error:', error);
                        alert('Failed to load order details');
                    }
                });
            });
            
            // Display order details in modal
            function displayOrderDetails(order) {
                document.getElementById('modalOrderId').textContent = order.orderId || order._id.slice(-6).toUpperCase();
                
                // Shipping Address
                const shippingAddress = `
                    <p><strong>${order.shippingAddress.name}</strong></p>
                    <p>${order.shippingAddress.addressLine1}</p>
                    ${order.shippingAddress.addressLine2 ? `<p>${order.shippingAddress.addressLine2}</p>` : ''}
                    <p>${order.shippingAddress.city}, ${order.shippingAddress.state} ${order.shippingAddress.pincode}</p>
                    <p>Phone: ${order.shippingAddress.phone}</p>
                `;
                document.getElementById('shippingAddress').innerHTML = shippingAddress;
                
                // Payment Method
                document.getElementById('paymentMethod').textContent = 
                    `${order.paymentInfo.status} (${order.paymentInfo.razorpayPaymentId ? 'Razorpay' : 'Unknown'})`;
                
                // Order Summary
                let orderSummaryHTML = '';
                order.items.forEach(item => {
                    orderSummaryHTML += `
                        <div style="display: flex; justify-content: space-between; margin-bottom: 8px;">
                            <span>${item.name} (x${item.quantity})</span>
                            <span>₹${(item.price * item.quantity).toLocaleString('en-IN')}</span>
                        </div>
                    `;
                });
                
                if (order.couponUsed) {
                    orderSummaryHTML += `
                        <div style="display: flex; justify-content: space-between; margin-bottom: 8px; color: var(--success);">
                            <span>Coupon Discount (${order.couponUsed.code})</span>
                            <span>-₹${order.couponUsed.discountAmount.toLocaleString('en-IN')}</span>
                        </div>
                    `;
                }
                
                orderSummaryHTML += `
                    <hr style="margin: 12px 0;">
                    <div style="display: flex; justify-content: space-between; font-weight: 600;">
                        <span>Total Amount</span>
                        <span>₹${order.totalAmount.toLocaleString('en-IN')}</span>
                    </div>
                `;
                
                document.getElementById('orderSummary').innerHTML = orderSummaryHTML;
                
                // Tracking Info
                const trackingSteps = document.getElementById('trackingSteps');
                trackingSteps.innerHTML = '';
                
                const statusOrder = ['Pending', 'Processing', 'Shipped', 'Out for Delivery', 'Delivered'];
                let currentStep = statusOrder.indexOf(order.orderStatus);
                if (currentStep === -1) currentStep = 0;
                
                // Use deliveryInfo.trackingHistory if available, otherwise create basic steps
                const trackingHistory = order.deliveryInfo?.trackingHistory || 
                    statusOrder.slice(0, currentStep + 1).map(status => ({
                        status,
                        date: new Date(),
                        remark: ''
                    }));
                
                trackingHistory.forEach((step, index) => {
                    const stepEl = document.createElement('li');
                    stepEl.className = 'tracking-step';
                    
                    if (index < currentStep) stepEl.classList.add('completed');
                    else if (index === currentStep) stepEl.classList.add('active');
                    
                    stepEl.innerHTML = `
                        <div class="step-title">${step.status}</div>
                        <div class="step-date">${new Date(step.date).toLocaleString('en-IN')}</div>
                        ${step.remark ? `<div class="step-remark">${step.remark}</div>` : ''}
                    `;
                    
                    trackingSteps.appendChild(stepEl);
                });
                
                // Update progress bar
                const progress = (currentStep / (statusOrder.length - 1)) * 100;
                document.getElementById('trackingProgress').style.width = `${progress}%`;
                
                // Coupon info
                if (order.couponUsed) {
                    document.getElementById('couponInfo').textContent = 
                        `${order.couponUsed.code} (₹${order.couponUsed.discountAmount.toLocaleString('en-IN')} off)`;
                }
                
                // Show modal
                modal.style.display = 'block';
            }
            
            // Cancel order functionality
            document.querySelectorAll('.cancel-order').forEach(button => {
                button.addEventListener('click', async function() {
                    const orderId = this.getAttribute('data-order-id');
                    if (confirm('Are you sure you want to cancel this order?')) {
                        try {
                            const response = await fetch(`/orders/${orderId}/cancel`, {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json'
                                }
                            });
                            
                            if (!response.ok) throw new Error('Failed to cancel order');
                            
                            const result = await response.json();
                            if (result.success) {
                                alert('Order cancelled successfully');
                                location.reload();
                            } else {
                                alert(result.message || 'Failed to cancel order');
                            }
                        } catch (error) {
                            console.error('Error:', error);
                            alert('An error occurred while cancelling the order');
                        }
                    }
                });
            });
        </script>
